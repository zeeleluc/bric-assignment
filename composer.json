{
    "name": "laravel/homestead",
    "description": "A virtual machine for web artisans.",
    "require": {
        "php": "^7.4",
        "symfony/console": "~4.4",
        "symfony/process": "~4.4",
        "symfony/yaml": "~5.1",
        "laravel/framework": "~5.8",
        "fideloper/proxy": "^4.4"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "phpunit/phpunit": "^9.5",
        "dms/phpunit-arraysubset-asserts": "^0.2.1",
        "barryvdh/laravel-ide-helper": "v2.8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Joe Ferguson",
            "email": "joe@joeferguson.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Laravel\\Homestead\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/homestead"
    ]
}
